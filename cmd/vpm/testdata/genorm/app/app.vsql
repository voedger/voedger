IMPORT SCHEMA 'mypkg1';
IMPORT SCHEMA 'mypkg2';

APPLICATION APP(
    USE mypkg1;
    USE mypkg2;
);

WORKSPACE MyAppWorkspace INHERITS mypkg1.MyWorkspace1(
    DESCRIPTOR MyAppWorkspaceDescriptor();
